{
  "common": {
    "Content-Type": "application/json",
    "Accept": "application/json",
    "User-Agent": "IRAS-ASR-Plus-Client/1.0"
  },
  "corppass_auth": {
    "Content-Type": "application/json",
    "Accept": "application/json"
  },
  "corppass_token": {
    "Content-Type": "application/x-www-form-urlencoded",
    "Accept": "application/json"
  },
  "api_submission": {
    "Content-Type": "application/json",
    "Accept": "application/json",
    "Authorization": "Bearer {{ACCESS_TOKEN}}"
  },
  "environments": {
    "sandbox": {
      "base_url": "https://apisandbox.iras.gov.sg/iras",
      "developer_portal": "https://apisandbox.iras.gov.sg/iras/devportal/sb"
    },
    "production": {
      "base_url": "https://apiservices.iras.gov.sg/iras",
      "developer_portal": "https://apiservices.iras.gov.sg/iras/devportal"
    }
  },
  "maintenance_hours": {
    "sandbox": {
      "daily": ["13:00-15:00", "20:00-22:00"],
      "weekly": "Wednesday 15:00-23:59"
    },
    "production": {
      "weekly": ["Wednesday 02:00-06:00", "Sunday 02:00-08:30"]
    }
  }
