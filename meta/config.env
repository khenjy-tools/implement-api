# API Configuration
BASE_URL=https://api.example.com
API_KEY=your_api_key_here
API_SECRET=your_secret_here

# Request Settings
TIMEOUT=30
RETRY_COUNT=3
RATE_LIMIT=100

# Debug Settings
DEBUG=false
LOG_LEVEL=INFO

# Optional: Database or Cache
DATABASE_URL=sqlite:///api_cache.db
REDIS_URL=redis://localhost:6379/0

# Authentication
AUTH_TYPE=bearer
# AUTH_TYPE=basic
# AUTH_TYPE=apikey

# Headers (JSON format)
CUSTOM_HEADERS={"User-Agent": "MyApp/1.0", "Accept": "application/json"}